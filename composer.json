{
    "name": "czim/hellodialog",
    "description": "HelloDialog e-mail marketing API handler",
    "keywords": [
        "hellodialog",
        "email-marketing"
    ],
    "homepage": "https://github.com/czim/hellodialog",
    "license": "MIT",
    "authors": [
        {
            "name": "Coen Zimmerman",
            "email": "coen@pixelindustries.com",
            "homepage": "http://www.pixelindustries.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "guzzlehttp/guzzle": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "scrutinizer/ocular": "~1.1",
        "orchestra/testbench": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "Czim\\HelloDialog\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Czim\\HelloDialog\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
